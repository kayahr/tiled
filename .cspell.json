{
    "$schema": "https://raw.githubusercontent.com/streetsidesoftware/cspell/master/cspell.schema.json",
    "version": "0.2",
    "files": [
        "**/*"
    ],
    "enabledLanguageIds": [
        "markdown",
        "typescript"
    ],
    "ignorePaths": [
        "node_modules",
        "coverage",
        "doc/api/**",
        "package-lock.json",
        "site",
        "git",
        "lib",
        "dist",
        "*.log",
        "*.tgz",
        "*.map.json",
        "*.template.json",
        "*.tileset.json",
        "*.tmj",
        "*.tsj",
        "*.tj"
    ],
    "words": [
        "apidoc",
        "ARGB",
        "backgroundcolor",
        "BOTTOMLEFT",
        "BOTTOMRIGHT",
        "compressionlevel",
        "copyfiles",
        "decompressors",
        "draworder",
        "editorsettings",
        "fillmode",
        "firstgid",
        "fontfamily",
        "fzstd",
        "halign",
        "hexsidelength",
        "hflip",
        "imageheight",
        "imagelayer",
        "imagewidth",
        "kayahr",
        "nextlayerid",
        "nextobjectid",
        "objectalignment",
        "objectgroup",
        "offsetx",
        "offsety",
        "pako",
        "parallaxoriginx",
        "parallaxoriginy",
        "parallaxx",
        "parallaxy",
        "pixelsize",
        "preferuntransformed",
        "Reimer",
        "renderorder",
        "repeatx",
        "repeaty",
        "staggeraxis",
        "staggerindex",
        "startx",
        "starty",
        "tilecount",
        "tiledversion",
        "tileheight",
        "tileid",
        "tilelayer",
        "tilemap",
        "tileoffset",
        "tilerendersize",
        "Tileset",
        "tilesets",
        "tilewidth",
        "tintcolor",
        "topdown",
        "TOPLEFT",
        "TOPRIGHT",
        "transparentcolor",
        "ungzip",
        "valign",
        "vflip",
        "wangid",
        "wangset",
        "wangsets",
        "wangtiles",
        "zstd"
    ],
    "language": "en",
    "enableFiletypes": [
        "json"
    ]
}
